{
  "$schema": "https://deno.land/x/denon@2.5.0/schema.json",
  "allow": [
    "net",
    "read",
    "env"
  ],
  "unstable": true,
  "scripts": {
    "start": {
      "watch": false,
      "cmd": "deno run main.ts",
      "desc": "run my app"
    },
    "dev": {
      "cmd": "deno run main.ts",
      "inspect": "127.0.0.1:9229",
      "desc": "run in dev mode (with debugger)"
    },
    "test": {
      "cmd": "deno test"
    }
  }
}